<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE form-validation PUBLIC
          "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.3.0//EN"
        "http://jakarta.apache.org/commons/dtds/validator_1_3_0.dtd">

<form-validation>
	<formset>
		<!--start_t_project_risk-->
		
		     <form name="tprojectRisk">
		     	<field property="riskId" depends="digits">
					<arg position="0" key="tprojectRisk.riskId" />
				</field>
			 	<field property="projectId" depends="required">	
		         <arg position="0" key="tprojectRisk.projectId" />		        
		        </field>
			 	<field property="prriskRate" depends="required,maxlength,number">	
		         <arg position="0" key="tprojectRisk.prriskRate" />		        
		         <arg position="1" name="maxlength" key="${var:maxlength}" resource="false" />
				 <var>
					<var-name>maxlength</var-name>
					<var-value>32</var-value>
				 </var>
		        </field>
			 	<field property="prrisk" depends="required,maxlength,number">	
		         <arg position="0" key="tprojectRisk.prrisk" />		        
		         <arg position="1" name="maxlength" key="${var:maxlength}" resource="false" />
				 <var>
					<var-name>maxlength</var-name>
					<var-value>32</var-value>
				 </var>
		        </field>
			 	<field property="prcompay" depends="required,maxlength">	
		         <arg position="0" key="tprojectRisk.prcompay" />		        
		         <arg position="1" name="maxlength" key="${var:maxlength}" resource="false" />
				 <var>
					<var-name>maxlength</var-name>
					<var-value>10</var-value>
				 </var>
		        </field>
			 	<field property="praudit" depends="required,maxlength">	
		         <arg position="0" key="tprojectRisk.praudit" />		        
		         <arg position="1" name="maxlength" key="${var:maxlength}" resource="false" />
				 <var>
					<var-name>maxlength</var-name>
					<var-value>3</var-value>
				 </var>
		        </field>
			 	<field property="prisReplacePay" depends="required,maxlength">	
		         <arg position="0" key="tprojectRisk.prisReplacePay" />		        
		         <arg position="1" name="maxlength" key="${var:maxlength}" resource="false" />
				 <var>
					<var-name>maxlength</var-name>
					<var-value>3</var-value>
				 </var>
		        </field>
			 	<field property="prreplacePay" depends="required,maxlength,number">	
		         <arg position="0" key="tprojectRisk.prreplacePay" />		        
		         <arg position="1" name="maxlength" key="${var:maxlength}" resource="false" />
				 <var>
					<var-name>maxlength</var-name>
					<var-value>32</var-value>
				 </var>
		        </field>
			 	<!-- <field property="prreplacePayTime" depends="required,maxlength">	
		         <arg position="0" key="tprojectRisk.prreplacePayTime" />		        
		         <arg position="1" name="maxlength" key="${var:maxlength}" resource="false" />
				 <var>
					<var-name>maxlength</var-name>
					<var-value>0</var-value>
				 </var>
		        </field> -->
			 	<field property="pradvice" depends="required,maxlength">	
		         <arg position="0" key="tprojectRisk.pradvice" />		        
		         <arg position="1" name="maxlength" key="${var:maxlength}" resource="false" />
				 <var>
					<var-name>maxlength</var-name>
					<var-value>200</var-value>
				 </var>
		        </field>
			 </form>
		<!--end_t_project_risk-->
</formset>
</form-validation>
 

 
